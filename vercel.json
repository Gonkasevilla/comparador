{
  "version": 2,
  "builds": [
    {
      "src": "api/compare.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/compare",
      "dest": "/api/compare.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "buildCommand": "pip install -r requirements.txt"
}
